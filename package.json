{
  "name": "reminder",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start-dev": "ng serve --open --proxy-config=proxy.dev.conf.js --port=1139",
    "start-local": "ng serve --open --proxy-config=proxy.local.conf.js --port=1140",
    "start-prod": "ng serve --aot --prod --open --proxy-config=proxy.conf.js --port=1141",
    "start-en": "ng serve --configuration=en --open --proxy-config=proxy.conf.js --port=1141",
    "start-fr": "ng serve --configuration=fr --open --proxy-config=proxy.conf.js --port=1142",
    "build": "ng build",
    "build-prod": "ng build --aot --prod",
    "build-prod-en": "ng build --base-href=\"https://ovnicap.github.io/Reminder/\" --output-path=dist/Reminder/en --aot --prod --i18n-file=src/i18n/messages.en.xlf --i18n-format=xlf --i18n-locale=en-US && node ./src/localize-index.js en ./dist/Reminder/en/index.html",
    "build-prod-fr": "ng build --base-href=\"https://ovnicap.github.io/Reminder-fr/\" --output-path=dist/Reminder/fr --aot --prod --i18n-file=src/i18n/messages.fr.xlf --i18n-format=xlf --i18n-locale=fr-FR && node ./src/localize-index.js fr ./dist/Reminder/fr/index.html",
    "publish-en": "ngh --dir=dist/Reminder/en --repo=git@github.com:OVNICap/Reminder.git",
    "publish-fr": "ngh --dir=dist/Reminder/en --repo=git@github.com:OVNICap/Reminder-fr.git",
    "publish": "npm run build-prod-en && npm run build-prod-fr && npm run publish-en && npm run publish-fr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n Reminder --i18n-format xlf --output-path i18n --i18n-locale fr && ng run Reminder:xliffmerge"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.801.2",
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "^8.1.1",
    "@angular/cli": "^8.1.2",
    "@angular/common": "^8.1.2",
    "@angular/compiler": "^8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/core": "^8.1.2",
    "@angular/forms": "^8.1.2",
    "@angular/language-service": "^8.1.2",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "@angular/router": "^8.1.2",
    "@mat-datetimepicker/core": "^2.0.1",
    "@ovnigames/framework": "^0.4.1",
    "apollo-angular": "^1.6.0",
    "apollo-angular-link-http": "^1.8.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "core-js": "^3.1.4",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "rxjs": "^6.5.2",
    "socket.io-client": "^2.2.0",
    "tslib": "^1.10.0",
    "typescript": "~3.4.5",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@types/jasmine": "^3.3.14",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.6.8",
    "@types/socket.io-client": "^1.4.32",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^5.1.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0"
  }
}
